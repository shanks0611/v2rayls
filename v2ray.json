{
  "api": {
    "services": [
      "HandlerService",
      "StatsService"
    ],
    "tag": "api"
  },
  "stats": {
  },
  "inbound": {
    "port": 8300,
    "protocol": "vmess",
    "settings": {
      "clients": []
    },
    "streamSettings": {
      "network": "ws",
	  "wsSettings": {
        "path":"/video"
      }	  
    },
    "tag": "proxy"
  },
  "inboundDetour": [{
    "listen": "0.0.0.0",
    "port": 8301,
    "protocol": "dokodemo-door",
    "settings": {
      "address": "0.0.0.0"
    },
    "tag": "api"
  }],
  "log": {
    "loglevel": "none",
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log"
  },
  "outbound": {
    "protocol": "freedom",
    "settings": {}
  },
  "routing": {
    "settings": {
      "rules": [{
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api",
        "type": "field"
      }]
    },
    "strategy": "rules"
  },
  "policy": {
    "levels": {
      "1": {
        "handshake": 2,
        "connIdle": 60,
        "uplinkOnly": 0,
        "downlinkOnly": 0,
        "statsUserUplink": true,
        "statsUserDownlink": true
      }
    }
  }
}
